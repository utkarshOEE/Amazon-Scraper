{
    "manifest_version": 2,
    "name": "Amazon history",
    "description": "Amazon scraper",
    "version": "0.01",
    "content_scripts": [
        {
            "matches": [
                "https://www.amazon.com/*"
            ],
            "js": [
                "js/lib/jquery-3.6.0.min.js",
                "js/app/content.js"
            ]
        }
    ],
    "background":{
        "scripts": ["js/lib/jquery-3.6.0.min.js", "js/app/background.js"]
    },
    "browser_action": {
        "default_popup": "views/popup.html"
    },
    "permissions": [
        "tabs"
    ]
}